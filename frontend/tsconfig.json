{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": ["dom", "es2019"],
    "module": "ES2015",
    "target": "es2019",
    "jsx": "react",
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
